{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.2.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-imask": "^6.4.3",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.7.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://192.168.0.10:3000 && electron .\"",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "copyright": "Thailes Pinheiro Estetaz",
    "mac": {
      "publish": {
        "provider": "github",
        "owner": "Thomashq",
        "repo": "Esthetic",
        "token": "token"
      }
    },
    "win": {
      "publish": {
        "provider": "github",
        "owner": "Thomashq",
        "repo": "Esthetic",
        "token": "token"
      }
    },
    "appId": "com.electron.esthetic",
    "productName": "Esthetic",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*",
      "src/**/*"
    ],
    "publish": {
      "provider": "github",
      "owner": "Thomashq"
    }
  }
}
